<PageFunction
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib" 
    x:Class="face_api_wpf_support.Views.AddItemPageFunction"
    x:TypeArguments="sys:Object"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:face_api_wpf_support.Views"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="300"
    Title="AddItemPageFunction">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>

        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="80"/>
            <RowDefinition Height="Auto"/>

        </Grid.RowDefinitions>
        <DockPanel Grid.Row="0" Grid.Column="0" Background="#00b294">
            <Button Content="Submit" Click="add_button_click" />
        </DockPanel>

        <StackPanel Grid.Row="1" Grid.Column="0" Background="Cyan">
            <TextBox x:Name="text_box"></TextBox>
        </StackPanel>

    </Grid>
</PageFunction>
